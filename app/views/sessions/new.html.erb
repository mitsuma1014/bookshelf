<h1>ログイン画面</h1>

<%= link_to "Top", :root %>
<%= link_to "新規登録", :new_user %>
<%= link_to "ログイン" ,login_path %>



<%= form_with scope: :session, local: true do |f| %>
  <div>
    <%= f.label :email, "メールアドレス" %>
    <%= f.text_field :email%>
  </div>
  <div>
    <%= f.label :password, "パスワード"%>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "ログインする"%>
<% end %>

